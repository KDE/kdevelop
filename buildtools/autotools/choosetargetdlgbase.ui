        <sender>activeTargetRadioButton</sender>
        <signal>toggled(bool)</signal>
        <receiver>chosenTargetGroupBox</receiver>
        <slot>setDisabled(bool)</slot>
    </connection>
    <connection>
        <sender>activeTargetRadioButton</sender>
        <signal>toggled(bool)</signal>
        <receiver>subprojectComboBox</receiver>
        <slot>setDisabled(bool)</slot>
    </connection>
    <connection>
        <sender>activeTargetRadioButton</sender>
        <signal>toggled(bool)</signal>
        <receiver>targetComboBox</receiver>
        <slot>setDisabled(bool)</slot>
    </connection>
</connections>
