#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Makefile is missing in folder \"%1\""
msgstr "Отсутствует Makefile в папке «%1»"

#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Problem while trying to resolve include paths for %1"
msgstr "Невозможно вычислить пути заголовочных файлов для %1"

#: makefileresolver/makefileresolver.cpp:310
#, kde-format
msgid "Cached: %1"
msgstr "В кэше: %1"

#: makefileresolver/makefileresolver.cpp:339
#, kde-format
msgid "Filename %1 seems to be malformed"
msgstr "Похоже, что имя файла %1 ошибочное"

#: makefileresolver/makefileresolver.cpp:472
#, kde-format
msgid "Failed to extract new working directory"
msgstr "Не удалось получить путь к новому рабочему каталогу"

#: makefileresolver/makefileresolver.cpp:472
#: makefileresolver/makefileresolver.cpp:501
#, kde-format
msgid "Output was: %1"
msgstr "Вывод: %1"

#: makefileresolver/makefileresolver.cpp:494
#: makefileresolver/makefileresolver.cpp:497
#, kde-format
msgid "Recursive make call failed"
msgstr "Ошибка рекурсивного вызова make"

#: makefileresolver/makefileresolver.cpp:494
#, kde-format
msgid "The parameter string \"%1\" does not seem to be valid. Output was: %2."
msgstr "Неправильно сформирована строка параметра «%1». Вывод: %2."

#: makefileresolver/makefileresolver.cpp:497
#, kde-format
msgid "The directory \"%1\" does not exist. Output was: %2."
msgstr "Каталог «%1» не существует. Вывод: %2."

#: makefileresolver/makefileresolver.cpp:501
#, kde-format
msgid "Malformed recursive make call"
msgstr "Неправильный рекурсивный вызов make"

#: makefileresolver/makefileresolver.cpp:513
#, kde-format
msgid "Could not extract include paths from make output"
msgstr "Не удалось выделить пути к заголовочным файлам из вывода make"

#: makefileresolver/makefileresolver.cpp:514
#, kde-format
msgid "Folder: \"%1\"  Command: \"%2\"  Output: \"%3\""
msgstr "Каталог: «%1»  Команда: «%2»  Вывод: «%3»"

