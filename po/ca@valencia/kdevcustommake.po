#: makefileresolver/makefileresolver.cpp:230
#, kde-format
msgid ""
"Tried include path resolution while another resolution process was still "
"running"
msgstr ""
"Intenta la resolució del camí cap als «include» mentre s'està executant un "
"altre procés de resolució"

#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Makefile is missing in folder \"%1\""
msgstr "Falta el Makefile en la carpeta «%1»"

#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Problem while trying to resolve include paths for %1"
msgstr ""
"Hi ha un problema en intentar identificar els camins dels «include» per a %1"

#: makefileresolver/makefileresolver.cpp:310
#, kde-format
msgid "Cached: %1"
msgstr "A la memòria cau: %1"

#: makefileresolver/makefileresolver.cpp:339
#, kde-format
msgid "Filename %1 seems to be malformed"
msgstr "El nom de fitxer %1 pareix estar amb format incorrecte"

#: makefileresolver/makefileresolver.cpp:472
#, kde-format
msgid "Failed to extract new working directory"
msgstr "No s'ha pogut extraure el nou directori de treball"

#: makefileresolver/makefileresolver.cpp:472
#: makefileresolver/makefileresolver.cpp:501
#, kde-format
msgid "Output was: %1"
msgstr "El resultat és: %1"

#: makefileresolver/makefileresolver.cpp:494
#: makefileresolver/makefileresolver.cpp:497
#, kde-format
msgid "Recursive make call failed"
msgstr "Ha fallat la crida recursiva a «make»"

#: makefileresolver/makefileresolver.cpp:494
#, kde-format
msgid "The parameter string \"%1\" does not seem to be valid. Output was: %2."
msgstr "La cadena del paràmetre «%1» no pareix vàlida. El resultat és: %2."

#: makefileresolver/makefileresolver.cpp:497
#, kde-format
msgid "The directory \"%1\" does not exist. Output was: %2."
msgstr "El directori «%1» no existix. El resultat és %2."

#: makefileresolver/makefileresolver.cpp:501
#, kde-format
msgid "Malformed recursive make call"
msgstr "La crida recursiva a «make» està amb format incorrecte"

#: makefileresolver/makefileresolver.cpp:513
#, kde-format
msgid "Could not extract include paths from make output"
msgstr ""
"No s'han pogut extraure els camins cap als «include» des de l'eixida de "
"«make»"

#: makefileresolver/makefileresolver.cpp:514
#, kde-format
msgid "Folder: \"%1\"  Command: \"%2\"  Output: \"%3\""
msgstr "Carpeta: «%1» Ordre: «%2» Resultat: «%3»"
