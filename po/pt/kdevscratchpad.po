#: scratchpad.cpp:104 scratchpadview.cpp:87
#, kde-format
msgctxt "@title:window"
msgid "Scratchpad"
msgstr "Quadro"

#: scratchpad.cpp:146 scratchpad.cpp:225 scratchpadjob.cpp:33
#, kde-format
msgctxt "prefix to distinguish scratch tabs"
msgid "scratch:%1"
msgstr "quadro:%1"

#: scratchpad.cpp:173
#, kde-format
msgid "Failed to remove scratch: %1"
msgstr "Não foi possível remover o quadro: %1"

#: scratchpad.cpp:180
#, kde-format
msgid "Failed to create scratch: Name already in use"
msgstr "Não foi possível criar o quadro: O nome já está a ser usado"

#: scratchpad.cpp:192
#, kde-format
msgid "Failed to create new scratch"
msgstr "Não foi possível criar um novo quadro"

#: scratchpad.cpp:201
#, kde-format
msgid "Failed to rename scratch: Names must not include path separator"
msgstr ""
"Não foi possível mudar o nome do quadro: Os nomes não devem incluir o "
"separador de pastas"

#: scratchpad.cpp:231
#, kde-format
msgid "Failed renaming scratch."
msgstr "Não foi possível mudar o nome do quadro."

#: scratchpadjob.cpp:57
#, kde-format
msgid "Running %1..."
msgstr "A executar o %1..."

#: scratchpadjob.cpp:73
#, kde-format
msgid "Process finished with exit code %1."
msgstr "O processo terminou com o código de saída %1."

#: scratchpadjob.cpp:80
#, kde-format
msgid "Failed to run scratch: %1"
msgstr "Não foi possível executar o quadro: %1"

#: scratchpadview.cpp:100
#, kde-format
msgid ""
"Scratchpad lets you quickly run and experiment with code without a full "
"project, and even store todos. Create a new scratch to start."
msgstr ""
"O Quadro permite-lhe executar e experimentar código rapidamente sem ter um "
"projecto completo ou ainda gravar tarefas. Crie um novo quadro para começar."

#: scratchpadview.cpp:114
#, kde-format
msgctxt "@info:tooltip"
msgid "Command to run this scratch. '$f' will expand to the scratch path."
msgstr ""
"Comando para executar este quadro. O '$f' será expandido para a localização "
"do quadro."

#: scratchpadview.cpp:137
#, kde-format
msgctxt "@action"
msgid "New Scratch"
msgstr "Novo Quadro"

#: scratchpadview.cpp:141
#, kde-format
msgctxt "@action"
msgid "Remove Scratch"
msgstr "Remover o Quadro"

#: scratchpadview.cpp:149
#, kde-format
msgctxt "@action"
msgid "Rename Scratch"
msgstr "Mudar o Nome do Quadro"

#: scratchpadview.cpp:158
#, kde-format
msgctxt "@action"
msgid "Run Scratch"
msgstr "Executar o Quadro"

#: scratchpadview.cpp:164
#, kde-format
msgctxt "@info:placeholder"
msgid "Filter..."
msgstr "Filtro..."

#: scratchpadview.cpp:204
#, kde-format
msgctxt "@title:window"
msgid "Create New Scratch"
msgstr "Criar um Novo Quadro"

#: scratchpadview.cpp:205
#, kde-format
msgctxt "@label:textbox"
msgid "Name for scratch file:"
msgstr "Indique o nome do ficheiro do quadro:"
