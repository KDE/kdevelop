#: scratchpad.cpp:104 scratchpadview.cpp:87
#, kde-format
msgctxt "@title:window"
msgid "Scratchpad"
msgstr "Scratchpad"

#: scratchpad.cpp:146 scratchpad.cpp:225 scratchpadjob.cpp:33
#, kde-format
msgctxt "prefix to distinguish scratch tabs"
msgid "scratch:%1"
msgstr "malneto:%1"

#: scratchpad.cpp:173
#, kde-format
msgid "Failed to remove scratch: %1"
msgstr "Malsukcesis forigi malneton: %1"

#: scratchpad.cpp:180
#, kde-format
msgid "Failed to create scratch: Name already in use"
msgstr "Malsukcesis krei scratch: Nomo jam uzata"

#: scratchpad.cpp:192
#, kde-format
msgid "Failed to create new scratch"
msgstr "Malsukcesis krei novan malneton"

#: scratchpad.cpp:201
#, kde-format
msgid "Failed to rename scratch: Names must not include path separator"
msgstr "Malsukcesis renomi scratch: Nomoj ne devas inkluzivi paddisigilon"

#: scratchpad.cpp:231
#, kde-format
msgid "Failed renaming scratch."
msgstr "Malsukcesis renomi scratch."

#: scratchpadjob.cpp:57
#, kde-format
msgid "Running %1..."
msgstr "Rulanta %1..."

#: scratchpadjob.cpp:73
#, kde-format
msgid "Process finished with exit code %1."
msgstr "Procezo finiĝis kun elirkodo %1."

#: scratchpadjob.cpp:80
#, kde-format
msgid "Failed to run scratch: %1"
msgstr "Malsukcesis ruli scratch: %1"

#: scratchpadview.cpp:100
#, kde-format
msgid ""
"Scratchpad lets you quickly run and experiment with code without a full "
"project, and even store todos. Create a new scratch to start."
msgstr ""
"Scratchpad ebligas vin rapide ruli kaj eksperimenti kun kodo sen plena "
"projekto, kaj eĉ konservi farendojn. Kreu novan malneton por komenci."

#: scratchpadview.cpp:114
#, kde-format
msgctxt "@info:tooltip"
msgid "Command to run this scratch. '$f' will expand to the scratch path."
msgstr "Komando por ruli ĉi tiun malneton. '$f' vastiĝos al la malnetvojo."

#: scratchpadview.cpp:137
#, kde-format
msgctxt "@action"
msgid "New Scratch"
msgstr "Nova Malneto"

#: scratchpadview.cpp:141
#, kde-format
msgctxt "@action"
msgid "Remove Scratch"
msgstr "Forigi Malneton"

#: scratchpadview.cpp:149
#, kde-format
msgctxt "@action"
msgid "Rename Scratch"
msgstr "Alinomi Malneton"

#: scratchpadview.cpp:158
#, kde-format
msgctxt "@action"
msgid "Run Scratch"
msgstr "Ruli Malneton"

#: scratchpadview.cpp:164
#, kde-format
msgctxt "@info:placeholder"
msgid "Filter..."
msgstr "Filtrilo..."

#: scratchpadview.cpp:204
#, kde-format
msgctxt "@title:window"
msgid "Create New Scratch"
msgstr "Krei Novan Malneton"

#: scratchpadview.cpp:205
#, kde-format
msgctxt "@label:textbox"
msgid "Name for scratch file:"
msgstr "Nomo por malneta dosiero:"
