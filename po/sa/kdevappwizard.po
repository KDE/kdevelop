#: appwizarddialog.cpp:23
#, kde-format
msgctxt "@title:window"
msgid "Create New Project"
msgstr "New Project रचयन्तु"

#: appwizarddialog.cpp:37
#, kde-format
msgctxt "@title:tab Page for general configuration options"
msgid "General"
msgstr "सामान्य"

#: appwizarddialog.cpp:39
#, kde-format
msgctxt "@title:tab Page for version control options"
msgid "Version Control"
msgstr "संस्करणनियन्त्रणम्"

#: appwizardplugin.cpp:69
#, kde-format
msgctxt "@action"
msgid "New from Template..."
msgstr "टेम्पलेट् तः नवीनम्..."

#: appwizardplugin.cpp:71
#, kde-format
msgctxt "@info:tooltip"
msgid "Generate a new project from a template"
msgstr "टेम्पलेट् तः नूतनं परियोजनां जनयन्तु"

#: appwizardplugin.cpp:72
#, kde-format
msgctxt "@info:whatsthis"
msgid ""
"This starts KDevelop's application wizard. It helps you to generate a "
"skeleton for your application from a set of templates."
msgstr ""
"एतेन KDevelop इत्यस्य अनुप्रयोगविजार्डः आरभ्यते । एतत् भवन्तं टेम्पलेट्-समूहात् भवतः "
"अनुप्रयोगाय कङ्कालं जनयितुं साहाय्यं करोति ।"

#: appwizardplugin.cpp:106
#, kde-format
msgid "Could not create project from template."
msgstr "टेम्पलेट् तः परियोजना निर्मातुं न शक्तवती ।"

#: appwizardplugin.cpp:134
#, kde-format
msgid "Please see the Version Control tool view."
msgstr "कृपया संस्करणनियन्त्रणसाधनदृश्यं पश्यन्तु ।"

#: appwizardplugin.cpp:136
#, kde-format
msgctxt "@title:window"
msgid "Version Control System Error"
msgstr "संस्करणनियन्त्रणप्रणालीदोषः"

#: appwizardplugin.cpp:152
#, kde-format
msgid "Could not initialize DVCS repository"
msgstr "DVCS भण्डारस्य आरम्भं कर्तुं न शक्तवान्"

#: appwizardplugin.cpp:161
#, kde-format
msgid "Could not check status of the DVCS repository"
msgstr "DVCS भण्डारस्य स्थितिं परीक्षितुं न शक्तवान्"

#: appwizardplugin.cpp:174
#, kde-format
msgid "Could not add files to the DVCS repository"
msgstr "DVCS भण्डारे सञ्चिकाः योजयितुं न शक्तवान्"

#: appwizardplugin.cpp:182
#, kde-format
msgid "Could not import project into %1."
msgstr "%1 मध्ये परियोजनां आयातयितुं न शक्तवान् ।"

#: appwizardplugin.cpp:199
#, kde-format
msgid "Could not import project"
msgstr "परियोजना आयातं कर्तुं न शक्तवान्"

#: appwizardplugin.cpp:207
#, kde-format
msgid "Could not checkout imported project"
msgstr "आयातितं परियोजनां चेकआउट् कर्तुं न शक्तवान्"

#: appwizardplugin.cpp:332
#, kde-format
msgid "Could not create new project"
msgstr "नूतनं परियोजनां निर्मातुं न शक्तवान्"

#: appwizardplugin.cpp:467
#, kde-format
msgid "The file %1 cannot be created."
msgstr "%1 इति सञ्चिका निर्मातुं न शक्यते ।"

