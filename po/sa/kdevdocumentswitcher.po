#: documentswitcherplugin.cpp:67
#, kde-format
msgctxt "@action"
msgid "Last Used Views"
msgstr "अन्तिमप्रयुक्तानि दृश्यानि"

#: documentswitcherplugin.cpp:70
#, kde-format
msgctxt "@info:whatsthis"
msgid "Opens a list to walk through the list of last used views."
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीं गन्तुं सूचीं उद्घाटयति ।"

#: documentswitcherplugin.cpp:71 documentswitcherplugin.cpp:79
#, kde-format
msgctxt "@info:tooltip"
msgid "Walk through the list of last used views"
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीं गच्छन्तु"

#: documentswitcherplugin.cpp:75
#, kde-format
msgctxt "@action"
msgid "Last Used Views (Reverse)"
msgstr "अन्तिमप्रयुक्तदृश्यानि (विपरीतानि)"

#: documentswitcherplugin.cpp:78
#, kde-format
msgctxt "@info:whatsthis"
msgid "Opens a list to walk through the list of last used views in reverse."
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीं विपरीतरूपेण गन्तुं सूचीं उद्घाटयति ।"
