#: scratchpad.cpp:215
#, kde-format
msgid "Failed to rename scratch: Names must not include path separator"
msgstr ""
"მონახაზის სახელის გადარქმევის შეცდომა: სახელი არ შეიძლება ბილიკის გამყოფს "
"შეიცავდეს"

#: scratchpad.cpp:256
#, kde-format
msgid "Failed renaming scratch."
msgstr "მონახაზის სახელის გადარქმევის შეცდომა."

#: scratchpad.cpp:299 scratchpadjob.cpp:33
#, kde-format
msgctxt "prefix to distinguish scratch tabs"
msgid "scratch:%1"
msgstr "მონახაზი: %1"

#: scratchpadjob.cpp:57
#, kde-format
msgid "Running %1..."
msgstr "%1-ის გაშვება..."

#: scratchpadjob.cpp:73
#, kde-format
